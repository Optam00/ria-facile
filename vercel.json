{
  "cleanUrls": true,
  "trailingSlash": false,
  "routes": [
    {
      "src": "/quiz",
      "dest": "/quiz.html",
      "status": 200
    },
    {
      "src": "/consulter",
      "dest": "/consulter.html",
      "status": 200
    },
    {
      "src": "/",
      "dest": "/index.html",
      "status": 200
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
}
