{
  "routes": [
    {
      "src": "/quiz",
      "dest": "/index.html",
      "headers": {
        "Link": "</quiz.html>; rel=preload; as=document"
      }
    },
    {
      "src": "/consulter",
      "dest": "/index.html",
      "headers": {
        "Link": "</consulter.html>; rel=preload; as=document"
      }
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
}
